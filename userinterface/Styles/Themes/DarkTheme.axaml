<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Backgrounds -->
	<SolidColorBrush x:Key="AppBackgroundBrush">#1E1E1E</SolidColorBrush>
	<SolidColorBrush x:Key="CardBackgroundBrush">#2D2D30</SolidColorBrush>
	<SolidColorBrush x:Key="ControlBackgroundBrush">#1E1E1E</SolidColorBrush>
	<SolidColorBrush x:Key="HoverBackgroundBrush">#252526</SolidColorBrush>
	<SolidColorBrush x:Key="SelectedBackgroundBrush">#0E639C</SolidColorBrush>

	<!-- Borders -->
	<SolidColorBrush x:Key="BorderBrush">#3F3F46</SolidColorBrush>
	<SolidColorBrush x:Key="FocusBorderBrush">#0E639C</SolidColorBrush>

	<!-- Text -->
	<SolidColorBrush x:Key="PrimaryTextBrush">#FFFFFF</SolidColorBrush>
	<SolidColorBrush x:Key="SecondaryTextBrush">#CCCCCC</SolidColorBrush>
	<SolidColorBrush x:Key="DisabledTextBrush">#666666</SolidColorBrush>

	<!-- Buttons -->
	<SolidColorBrush x:Key="PrimaryButtonBrush">#0E639C</SolidColorBrush>
	<SolidColorBrush x:Key="PrimaryButtonHoverBrush">#1177BB</SolidColorBrush>
	<SolidColorBrush x:Key="PrimaryButtonPressedBrush">#0D5A94</SolidColorBrush>
	<SolidColorBrush x:Key="DisabledButtonBrush">#4A4A4A</SolidColorBrush>
	<SolidColorBrush x:Key="DangerButtonBrush">#DC2626</SolidColorBrush>
	<SolidColorBrush x:Key="DangerButtonHoverBrush">#EF4444</SolidColorBrush>
	<SolidColorBrush x:Key="DangerButtonPressedBrush">#B91C1C</SolidColorBrush>
	<SolidColorBrush x:Key="RenameButtonBrush">#FFA500</SolidColorBrush>
	<SolidColorBrush x:Key="RenameButtonHoverBrush">#FFA500</SolidColorBrush>
	<SolidColorBrush x:Key="ButtonIconOnHoverBrush">#FFFFFF</SolidColorBrush>
	<SolidColorBrush x:Key="AccentBrush">#0E639C</SolidColorBrush>
	<SolidColorBrush x:Key="AccentHoverBrush">#1177BB</SolidColorBrush>
	<SolidColorBrush x:Key="AccentForegroundBrush">#FFFFFF</SolidColorBrush>
	<SolidColorBrush x:Key="SuccessButtonBrush">#059669</SolidColorBrush>
	<SolidColorBrush x:Key="SuccessButtonHoverBrush">#10B981</SolidColorBrush>
	<SolidColorBrush x:Key="SuccessButtonPressedBrush">#047857</SolidColorBrush>

	<!-- Secondary Highlight (Orange) -->
	<SolidColorBrush x:Key="SecondaryHighlightBrush">#D97706</SolidColorBrush>
	<SolidColorBrush x:Key="SecondaryHighlightHoverBrush">#F59E0B</SolidColorBrush>
	<SolidColorBrush x:Key="SecondaryHighlightPressedBrush">#B45309</SolidColorBrush>
	
	<!-- Status Colors -->
	<SolidColorBrush x:Key="ActiveBrush">#22C55E</SolidColorBrush>
	<SolidColorBrush x:Key="ActiveHoverBrush">#22C55E33</SolidColorBrush>
	<SolidColorBrush x:Key="ActiveBorderHoverBrush">#16A34A</SolidColorBrush>
	<SolidColorBrush x:Key="InactiveBrush">#9CA3AF</SolidColorBrush>
	
	<!-- UI Effects -->
	<SolidColorBrush x:Key="ShadowBrush">#00000040</SolidColorBrush>
	<SolidColorBrush x:Key="ToastShadowBrush">#33000000</SolidColorBrush>
	<SolidColorBrush x:Key="ModalShadowBrush">#40000000</SolidColorBrush>
	<SolidColorBrush x:Key="CardHoverBackgroundBrush">#353537</SolidColorBrush>
	<SolidColorBrush x:Key="ModalBackgroundBrush">#AA000000</SolidColorBrush>
	
	<!-- Box Shadows -->
	<BoxShadows x:Key="CardShadow">0 2 8 0 #00000040</BoxShadows>
	<BoxShadows x:Key="ToastShadow">0 4 12 0 #33000000</BoxShadows>
	<BoxShadows x:Key="ModalShadow">0 16 32 0 #40000000</BoxShadows>
</ResourceDictionary>
